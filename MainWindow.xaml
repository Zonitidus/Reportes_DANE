<Window x:Class="ReportesDane.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReportesDane"
        mc:Ignorable="d"
        Title="DANE" Height="450" Width="800" ResizeMode="NoResize" Cursor="Pen">
    <Grid Cursor="Pen">
        <Button x:Name="Open_file" Content="Open file" HorizontalAlignment="Left" Margin="30,390,0,0" VerticalAlignment="Top" Click="Open_file_Click"/>
        <Label x:Name="label" Content="Path:" HorizontalAlignment="Left" Margin="113,387,0,0" VerticalAlignment="Top" Width="660"/>
        <ScrollViewer x:Name="tableScroll" Margin="30,32,400,98">
            <DataGrid x:Name="dataDANE" Height="269" Width="323"/>
        </ScrollViewer>
        <ScrollViewer x:Name="diagramScroll" Margin="403,32,27,98"/>

    </Grid>
</Window>
